<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Recepcjonistka" table="Recepcjonistka" lazy="false">
		<id name="id" column="Id" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="konto" column="KontoID" class="model.Konto"
			cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
		<many-to-one name="plan_pracy" column="Plan_pracyID"
			class="model.Plan_pracy" cascade="save-update,lock" unique="true" not-null="true"
			lazy="proxy">
		</many-to-one>
		<many-to-one name="osoba" column="OsobaID" class="model.Osoba"
			cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>

		<set name="wizyta" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="RecepcjonistkaId" not-null="false" />
			<one-to-many class="model.Wizyta" />
		</set>
		<set name="platnosc" lazy="true" cascade="save-update,lock"
			inverse="true">
			<key column="RecepcjonistkaId" not-null="true" />
			<one-to-many class="model.Platnosc" />
		</set>
	</class>
</hibernate-mapping>
