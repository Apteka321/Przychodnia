<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Produkt" table="Produkt" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nazwa" column="Nazwa" type="string" length="255" not-null="true" lazy="false"/>
		<property name="cena" column="Cena" type="big_decimal" precision="10" scale="2" not-null="true" lazy="false"/>
		<property name="ilosc" column="Ilosc" type="integer" length="10" not-null="true" lazy="false"/>
		<set name="zamowienia" table="Produkt_Zamowienia" lazy="true" cascade="save-update,lock">
			<key column="ProduktID" not-null="true"/>
			<many-to-many column="ZamowieniaID" class="model.Zamowienia">
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>
