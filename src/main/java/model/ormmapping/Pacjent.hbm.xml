<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Pacjent" table="Pacjent" lazy="false">
		<id name="PESEL" column="PESEL" type="string">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="konto" column="KontoID" class="model.Konto" cascade="save-update,lock" unique="true" not-null="false" lazy="proxy">
		</many-to-one>
		<many-to-one name="osoba" column="OsobaID" class="model.Osoba" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
		<set name="wizyta" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="`PESEL pacjenta`" not-null="true"/>
			<one-to-many class="model.Wizyta"/>
		</set>
	</class>
</hibernate-mapping>
